<?xml version="1.0" encoding="utf-8"?><TableLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content">    <TableRow android:background="@color/white">        <TextView            android:id="@+id/nomorItem"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="3"            android:gravity="center"            android:padding="10sp"            android:text="1"            android:textColor="@color/black"            android:textSize="9dp"            android:textStyle="bold" />        <TextView            android:id="@+id/tanggalTable"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="4"            android:gravity="center"            android:padding="10sp"            android:text="12/2/2022"            android:textColor="@color/black"            android:textSize="9sp"            android:textStyle="bold" />        <TextView            android:id="@+id/namaPlatTable"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="4"            android:gravity="center"            android:padding="10sp"            android:text="BM 1232 VC"            android:textColor="@color/black"            android:textSize="9dp"            android:textStyle="bold" />        <TextView            android:id="@+id/penerimaanTable"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="4"            android:gravity="center"            android:padding="10sp"            android:text="100982"            android:textColor="@color/black"            android:textSize="9sp"            android:textStyle="bold" />        <TextView            android:id="@+id/sortTable"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="4"            android:gravity="center"            android:padding="10sp"            android:text="240"            android:textColor="@color/black"            android:textSize="9sp"            android:textStyle="bold" />        <TextView            android:id="@+id/produksiTable"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="4"            android:gravity="center"            android:padding="10sp"            android:text="9022"            android:textColor="@color/black"            android:textSize="9dp"            android:textStyle="bold" />    </TableRow></TableLayout>